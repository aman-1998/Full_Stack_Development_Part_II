package personal.learning.cars;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Component;

import personal.learning.interfaces.Car;

@Component
public class Thar implements Car {

	
	Engine engine;
	
	@Autowired
	public Thar() {
		engine.setTorque("300 Nm");
	}

	@Override
	public String specification() {
		return "Rough and tough SUV from Mahindra. Torque of the engine is "+engine.getTorque();
	}
	
	public String size() {
		return "Large in size";
	}
}
